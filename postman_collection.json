    {
      "name": "Autenticação",
      "item": [
        {
          "name": "Professor Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "pm.environment.set(\"PROFESSOR_TOKEN\", jsonData.token);",
                  "pm.environment.set(\"PROFESSOR_ID\", jsonData.id);"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"nome\": \"professor1\",\n  \"senha\": \"senha123\"\n}"
            },
            "url": {
              "raw": "{{BASE_URL}}/api/professores/login",
              "host": [
                "{{BASE_URL}}"
              ],
              "path": [
                "api",
                "professores",
                "login"
              ]
            },
            "description": "Realiza login de um professor e retorna um token JWT válido por 24h"
          },
          "response": []
        },
        {
          "name": "Aluno Login",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "var jsonData = pm.response.json();",
                  "pm.environment.set(\"ALUNO_TOKEN\", jsonData.token);",
                  "pm.environment.set(\"ALUNO_ID\", jsonData.id);"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json"
              }
            ],
            "body": {
              "mode": "raw",
              "raw": "{\n  \"nome\": \"aluno1\",\n  \"senha\": \"senha123\"\n}"
            },
